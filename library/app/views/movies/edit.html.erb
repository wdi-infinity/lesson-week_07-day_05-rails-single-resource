<h1> Edit a movie </h1>

<%= form_with scope: :movie, url: '/movies', local: true do |form| %>
    <p> 

    <%= form.text_field :director  %>
    </p>
    <p> 
    <%= form.text_field :title %>
    </p>
    <p> 
    <%= form.number_field :rating %>
    </p>
    <p> 
    <%= form.submit %>
    </p>
    <%end%>