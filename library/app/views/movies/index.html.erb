<h1>Hello</h1>
<h2> <%= link_to "Add new movie",  new_movie_path %></h2>

<% @movies.each do |movie|  %>
    <h2> <%= movie.title %> </h2>
    <p><%= link_to 'view' , movie_path(movie)%></p>

    <p><%= link_to 'Delete', movie_path(movie), method: :delete,
     data: {confirm: "Are you sure"} %></p>

     <p><%= link_to 'Edit', edit_movie_path(movie), 
     data: {confirm: "Are you sure to edit"} %></p>
<%end%>